{
  "rewrites": [
    {
      "source": "/_next/:path*",
      "destination": "https://blog-ssr-phi.vercel.app/_next/:path*"
    },
    {
      "source": "/blogs",
      "destination": "https://blog-ssr-phi.vercel.app/blogs"
    },
    {
      "source": "/blogs/:path*",
      "destination": "https://blog-ssr-phi.vercel.app/blogs/:path*"
    },
    {
      "source": "/((?!blogs|_next).*)",
      "destination": "/index.html"
    }
  ]
}