{
  "rewrites": [
    {
      "source": "/blogs",
      "destination": "https://blog-ssr-phi.vercel.app"
    },
    {
      "source": "/blogs/:path*",
      "destination": "https://blog-ssr-phi.vercel.app/:path*"
    },
    {
      "source": "/((?!blogs).*)",
      "destination": "/index.html"
    }
  ]
}